                onAction="#handleCancelButton" text="Cancel"/>
        <TextArea fx:id="informatiiNotaTextArea" layoutX="119.0" layoutY="58.0" prefHeight="200.0" prefWidth="335.0"/>
        <Label fx:id="titluLabel" layoutX="247.0" layoutY="24.0" text="Informatii nota"/>
    </children>
</AnchorPane>
