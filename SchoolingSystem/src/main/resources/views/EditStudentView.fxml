<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" stylesheets="@../css/style.css" fx:controller="controller.EditStudentController">
    <children>
        <TextField fx:id="idTextField" layoutX="129.0" layoutY="44.0"/>
        <TextField fx:id="numeTextField" layoutX="129.0" layoutY="89.0"/>
        <TextField fx:id="prenumeTextField" layoutX="129.0" layoutY="134.0"/>
        <TextField fx:id="grupaTextField" layoutX="129.0" layoutY="180.0"/>
        <TextField fx:id="profesorTextField" layoutX="129.0" layoutY="224.0"/>
        <TextField fx:id="emailTextField" layoutX="129.0" layoutY="267.0"/>
        <Label fx:id="idLabel" layoutX="37.0" layoutY="44.0" text="ID"/>
        <Label fx:id="numeLabel" layoutX="37.0" layoutY="89.0" text="Nume"/>
        <Label fx:id="prenumeLabel" layoutX="37.0" layoutY="134.0" text="Prenume"/>
        <Label fx:id="grupaLabel" layoutX="37.0" layoutY="180.0" text="Grupa"/>
        <Label fx:id="profesorLabel" layoutX="37.0" layoutY="224.0" text="Profesor"/>
        <Label fx:id="emailLabel" layoutX="37.0" layoutY="267.0" text="Email"/>
        <Button fx:id="adaugaButton" layoutX="81.0" layoutY="322.0" mnemonicParsing="false" onAction="#handleSaveButton"
                prefHeight="26.0" prefWidth="45.0" text="Save"/>
        <Button fx:id="exitButton" layoutX="179.0" layoutY="322.0" mnemonicParsing="false" onAction="#handleExitButton"
                prefHeight="26.0" prefWidth="48.0" text="Exit"/>
    </children>
</AnchorPane>
